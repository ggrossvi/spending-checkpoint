{
  "name": "Spending Checkpoint",
  "description": "Chrome extension that pops up before purchasing something",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.target.com/p/*",
        "https://www.amazon.com/gp/cart*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}